{
	"manifest_version": 2, 
	"name": "BET-365 Extension", 
	"version": "1.0", 
	"description": "BET-365 extension...", 
	"browser_action": { 
		"default_icon": "/logo.jpg" 
	},
	"permissions": [ 
		"https://*/",
		"bookmarks",
		"tabs",
		"history"
	],
	"content_scripts": [
		{
			"matches": ["https://www.448365365.com/*"],
			"js": ["jquery_1.9.1.js","bet365.js"]
		}
	]
}